@model IEnumerable<PassportTracker.Models.PassportVM>


<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
    <ul class="nav navbar-nav">
        @if ((ViewData["InOwnerRole"] == "true") || (ViewData["InAuthorRole"] == "true") || (ViewData["InCountryRole"] == "true"))
        {
            <li>@Html.ActionLink("Surrender Passport", "Create", "PassportForm", new { SPHostUrl = SharePointContextFilterAttribute.sphosturl }, new { @class = "btn btn-primary"})</li>
        }
      
    </ul>  
</div>
 

<h2>Documents</h2>

<table id="example" border="0" cellpadding="0" cellspacing="0">
    <thead>
        <tr >
            <th>ViewColumn</th>
            <th>Initial Location</th>
            <th>Passport Number</th>
            <th>Name</th>
            <th>Date of Birth</th>
            <th>Case Id</th>
            <th>Date Handed In</th>
            <th>Date Returned</th>
            @*<th>Id</th>*@
        </tr>
    </thead>

</table>

